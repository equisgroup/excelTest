{
  "ConnectionStrings": {
    "TestDatabase": "Filename=database-test.db;Connection=shared",
    "ProdDatabase": "Filename=database-prod.db;Connection=shared"
  },
  "App": {
    "DefaultMode": "Test",
    "ReportsOutputDirectory": "./Reportes"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/app-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
